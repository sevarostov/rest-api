{
    "openapi": "3.0.0",
    "info": {
        "title": "Rest API for catalog",
        "version": "1"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "User"
                ],
                "operationId": "8dcb70df1020986038d098cc08d05dae",
                "requestBody": {
                    "description": "User's credentials",
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "vito49@example.net"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Get a JWT via given credentials"
                    }
                }
            }
        },
        "/api/buildings": {
            "get": {
                "tags": [
                    "Buildings"
                ],
                "summary": "Get list of buildings",
                "description": "Returns list of buildings",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/companies/building/{id}": {
            "get": {
                "tags": [
                    "Company"
                ],
                "summary": "Get list of companies by building",
                "description": "Returns list of companies by building",
                "operationId": "getByBuilding",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "building id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "1"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "User",
            "description": "User"
        },
        {
            "name": "Buildings",
            "description": "Buildings"
        },
        {
            "name": "Company",
            "description": "Company"
        }
    ]
}