{
    "openapi": "3.0.0",
    "info": {
        "title": "Rest API for catalog",
        "version": "1"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "User"
                ],
                "operationId": "8dcb70df1020986038d098cc08d05dae",
                "requestBody": {
                    "description": "User's credentials",
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "leopold.kuhic@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Get a JWT via given credentials"
                    }
                }
            }
        },
        "/api/auth/buildings": {
            "get": {
                "tags": [
                    "Buildings"
                ],
                "summary": "Get list of buildings",
                "description": "Returns list of buildings",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/auth/companies/building/{id}": {
            "get": {
                "tags": [
                    "Company"
                ],
                "summary": "Get list of companies by building",
                "description": "Returns list of companies by building",
                "operationId": "getByBuilding",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "building id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "1"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/auth/companies/rubric/{id}": {
            "get": {
                "tags": [
                    "Company"
                ],
                "summary": "Get list of companies by rubric",
                "description": "Returns list of companies by rubric",
                "operationId": "getByRubric",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "rubric id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "1"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/auth/companies/map/point/{latitude}/{longitude}/{type}": {
            "get": {
                "tags": [
                    "Company"
                ],
                "summary": "Get list of companies by map point",
                "description": "Returns list of companies by map point",
                "operationId": "getByMapPoint",
                "parameters": [
                    {
                        "name": "latitude",
                        "in": "path",
                        "description": "latitude",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "47.897709"
                        }
                    },
                    {
                        "name": "longitude",
                        "in": "path",
                        "description": "longitude",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "40.082366"
                        }
                    },
                    {
                        "name": "type",
                        "in": "path",
                        "description": "type of search: radius or rectangle",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": "1"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "apiKey": []
                    }
                ]
            }
        }
    },
    "tags": [
        {
            "name": "User",
            "description": "User"
        },
        {
            "name": "Buildings",
            "description": "Buildings"
        },
        {
            "name": "Company",
            "description": "Company"
        }
    ],
    "components": {
        "securitySchemes": {
            "apiKey": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}